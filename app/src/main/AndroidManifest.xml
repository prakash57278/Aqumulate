<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.intern.aqumulate.aqumulate">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Transaction"
            android:label="@string/Transaction"></activity>
        <activity
            android:name=".Portfolio"
            android:label="Portfolio"></activity>
        <activity
            android:name=".Help"
            android:label="Help"></activity>
        <activity
            android:name=".EditAccount"
            android:label="Edit Account"></activity>
        <activity
            android:name=".AddAccount"
            android:label="Add Account"></activity>
        <activity android:name=".AssestsLiabilities"></activity>
    </application>

</manifest>